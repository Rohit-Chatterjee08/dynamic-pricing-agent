{
  "name": "shopify-app-production",
  "version": "1.0.0",
  "description": "Production-ready Shopify Remix app with multi-tenant support",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "build": "shopify app build",
    "dev": "shopify app dev",
    "config:link": "shopify app config link",
    "config:use": "shopify app config use",
    "deploy": "shopify app deploy",
    "docker:build": "docker build -t shopify-app .",
    "docker:run": "docker run -p 3000:3000 --env-file .env shopify-app",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset --force",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "healthcheck": "node scripts/healthcheck.js",
    "verify-production": "node scripts/verify-production.js",
    "start": "remix-serve ./build/server/index.js",
    "prestart": "npm run db:migrate && npm run db:generate"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@remix-run/node": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "@remix-run/serve": "^2.4.1",
    "@shopify/app-bridge": "^4.1.2",
    "@shopify/app-bridge-react": "^4.1.2",
    "@shopify/polaris": "^12.14.0",
    "@shopify/shopify-api": "^8.1.1",
    "@shopify/shopify-app-remix": "^2.8.0",
    "@shopify/shopify-app-session-storage-prisma": "^4.0.5",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "isbot": "^4.4.0",
    "morgan": "^1.10.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.6.12",
    "remix": "^2.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.4.1",
    "@shopify/cli": "^3.54.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^1.1.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
